sole.log(this.photoBook);\n        })\n            .catch(err=> console.log(err));\n    }\n\n    getPhotos(queryParam) {\n        const client = createClient('563492ad6f917000010000017c2d8937bbf246fe90de28ef275466fe');\n        const query = queryParam;\n        console.log(queryParam);\n        client.photos.search({ query, perPage: 10 }).then(photos => {\n            this.searchPhoto=true;\n            //console.log(photos.photos[0].url);\n            this.photoBook= photos.photos;\n            console.log(this.photoBook);\n        })\n            .catch(err=> console.log(err));\n    }\n\n    searchPhotos(e: any) {\n        e.preventDefault();\n        this.getPhotos(this.searchKey);\n        this.searchKey=\"\";\n    }\n\n    sort(param: string) {\n        console.log(param);\n\n        if(param==\"height\")\n        {\n            //const sortedPhotos=this.photoBook.slice(0,10).sort((a,b) =>(a.height > b.height) ? 1 : -1); \n            this.photoBook=this.photoBook.slice(0,10).sort((a,b) =>(a.height > b.height) ? 1 : -1);\n            console.log(this.photoBook);\n        }\n        else {\n            this.photoBook=this.photoBook.slice(0,10).sort((a,b) =>(a.width > b.width) ? 1 : -1);\n            console.log(this.photoBook);\n        }\n    }\n\n    mounted() {\n        $(function() {\n            let selectedClass = \"\";\n            $(\".filter\").click(function(){\n            selectedClass = $(this).attr(\"data-rel\");\n            $(\"#gallery\").fadeTo(100, 0.1);\n            $(\"#gallery div\").not(\".\"+selectedClass).fadeOut().removeClass('animation');\n            setTimeout(function() {\n            $(\".\"+selectedClass).fadeIn().addClass('animation');\n            $(\"#gallery\").fadeTo(300, 1);\n            }, 300);\n            });\n            });\n    }\n}\n</script>\n\n\n<style scoped>\n   p {\n    margin-top: 100px;\n    margin-bottom: 1rem;\n    text-align: center;\n    }\n\n    .gallery {\n        -webkit-column-count: 3;\n        -moz-